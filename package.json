{
  "name": "@vrsim/earth-engine-ol",
  "version": "0.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && rollup --bundleConfigAsCjs -c rollup.config.mjs --environment MODE:prod && tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "clean": "rimraf -rf dist/",
    "prepack": "npm run build",
    "copy": "copyfiles -u 1 src/**/*.glsl earth-engine",
    "tsc": "yarn clean && tsc && yarn copy",
    "doc": "npx typedoc --options ./typedoc.json"
  },
  "author": "gkc",
  "peerDependencies": {
    "heatmap.js": "^2.0.5",
    "mitt": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "ol-wind": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-url": "^8.0.2",
    "@turf/turf": "^6.5.0",
    "@types/heatmap.js": "^2.0.37",
    "@types/lodash": "^4.17.20",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.34.0",
    "ol": "^7.5.2",
    "rimraf": "^4.1.2",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-shader": "^0.0.3",
    "sass": "^1.58.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "5.4",
    "vite": "^4.1.1",
    "vite-plugin-string": "^1.2.1",
    "vue-tsc": "^1.0.24"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "sideEffects": [
    "dist/index.css",
    "dist/index.es.css",
    "dist/index.cjs.css",
    "dist/index.global.css"
  ]
}
